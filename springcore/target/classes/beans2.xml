<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="employee"
		class="com.bcits.springcore.beans.EmployeeBean1">
		<property name="name" value="Ashin" />
		<property name="age" value="23" />
		<property name="departmentBean" ref="departmentBean" />
	</bean>


	<bean id="departmentBean"
		class="com.bcits.springcore.beans.DepartmentBean">
		<property name="deptId" value="11 " />
		<property name="deptName" value="SD" />
	</bean>

</beans>